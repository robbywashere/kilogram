  "scripts": {
    "start": "node ./server/index.js",
    "py:install": "pip3 install -r ./android/requirements.txt",
    "dev:tabs": "export LOG_LEVEL=3 && ./scripts/tab yarn run minio:up & ./scripts/tab yarn run dev & ./scripts/tab yarn run crx:start & ./scripts/tab yarn run device-node",
    "crx:start": "export PORT=3001 && react-scripts start",
    "crx:build": "react-scripts build",
    "crx:test": "react-scripts test --env=jsdom",
    "crx:eject": "react-scripts eject",
    "minio:up": "minio server \"$HOME/minio-data/\"",
    "minio:bucket:up": "mc mb local/uploads",
    "minio:events:up": "mc events add local/uploads arn:minio:sqs::1:postgresql --events put,delete",
    "minio:events:down": "mc events remove local/uploads arn:minio:sqs::1:postgresql --force",
    "dev:auto": "NODE_ENV=development && nodemon ./server/index.js",
    "dev": "export LOG_LEVEL=3 && NODE_ENV=development && nodemon ./server/index.js",
    "eslint": "eslint",
    "sync:seed": " node ./db/_sync.js && node ./db/seeders/_up.js",
    "dev:seeds:refresh": "export NODE_ENV='development' && yarn run seeds:down && yarn run seeds:up",
    "prod:seeds:refresh": "read -p \"Are you sure? \" -n 1 -r && if [[ $REPLY =~ ^[Yy]$ ]]; then export NODE_ENV='production' && yarn run seeds:down && yarn run seeds:up;fi",
    "seeds:up": "node ./db/seeders/_up.js",
    "seeds:create": "node ./db/seeders/_create.js",
    "seeds:down": "node ./db/seeders/_down.js",
    "e:dev": "export NODE_ENV='development' && ./.npmenv",
    "e:test": "export NODE_ENV='test' && ./.npmenv",
    "e:prod": "export NODE_ENV='production' && /.npmenv",
    "db:func:dump": "node ./db/_funcdump.js",
    "db:func:up": "node ./db/_funcup.js",
    "db:triggers": "node ./db/postgres-triggers/_getTriggers.js",
    "db:sync": "node ./db/_sync.js",
    "db:sync:force": "node ./db/_syncForce.js",
    "db:up": "node ./db/_up.js",
    "db:schema:dump": "node ./db/schema/_dump",
    "db:schema:dump:compare": "node ./db/schema/_dump_compare",
    "db:down": "node ./db/_down.js",
    "db:mig:down": "node ./db/_migdown.js",
    "db:schema:up": "node ./db/_schemaup.js",
    "db:mig:up": "node ./db/_migup.js",
    "test:clr": "./scripts/testrunner --reporter mocha-clearscreen-reporter --watch",
    "test:clr:verbose": "./scripts/testrunner --reporter mocha-clearscreen-reporter --watch",
    "test:watch": "mocha  --require ./test/setup.js  --recursive \"./test/**/*.test.js\"  --watch",
    "test": "./scripts/testrunner",
    "test:nyc": " nyc ./scripts/testrunner",
    "test:e2e": "mocha ./e2e/e2e.test.js",
    "test:e2e:device": "export RUN_E2E_ON_DEVICE=true && mocha ./e2e/e2e.test.js",
    "pg:session:up": "psql postgres < ./node_modules/connect-pg-simple/table.sql",
    "moo": "cowsay moo",
    "pwd": "pwd",
    "device-node": "node ./engine/run",
    "env": "echo $NODE_ENV",
    "bootstrap": "yarn db:up && yarn db:schema:up && pip install -r ./android/requirements.txt"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run e:dev db:schema:dump:compare"
    }
  },
  "proxy": "http://127.0.0.1:3000/",
  "devDependencies": {
    "babel-eslint": "^8.0.3",
